List<Map<String, String>> errorResponse = new ArrayList<>();

{
    Map<String, String> errorMap = new HashMap<>();
    errorMap.put("jiraErrorResponse", "Project Doesn't Exist");
    errorResponse.add(errorMap);



List<Map<String, String>> finalResponse = new ArrayList<>();




public void buildFinalResponse(Map<String, String> taInput, boolean updateSuccess) {
    Map<String, String> responseMap = new LinkedHashMap<>();

    Map<String, String> fieldLabels = Map.of(
        "planviewID", "Planview ID",
        "asa_approval_date", "ASA approval date",
        "architect_soeid", "ASA: Architect SOEID",
        "asa_override_request_date", "ASA override request date",
        "asa_override_decision_date", "ASA override decision date",
        "asa_override_appr_soeid", "ASA: Override Approver SOEID",
        "asa_architectural_significance", "Architecturally Significant",
        "asa_status", "ASA: Status"
    );

    for (Map.Entry<String, String> entry : fieldLabels.entrySet()) {
        String key = entry.getKey();
        String label = entry.getValue();
        String value = taInput.getOrDefault(key, "");

        if (value == null || value.isBlank()) {
            responseMap.put(label, "No Update");
        } else {
            responseMap.put(label, updateSuccess ? "Succesfully Updated" : "Not Succesfully Updated");
        }
    }

    finalResponse.add(responseMap);
}